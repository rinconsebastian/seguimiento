@model seguimiento.Models.Periodo

@{
    ViewData["Title"] = "Panel de control";
    ViewData["Subtitle"] = "Periodos";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
<h4>Detalles periodo</h4>

<div class="form-group row">
    <label asp-for="orden" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">@Html.DisplayFor(model => model.orden)</p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="nombre" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">@Html.DisplayFor(model => model.nombre)</p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="tipo" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">@Html.DisplayFor(model => model.tipo)</p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="calculo" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.calculo ? "Si" : "No")
        </p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="cargado" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.cargado ? "Si" : "No")
        </p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="Ocultar" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.Ocultar ? "Si" : "No")
        </p>
    </div>
</div>


<div class="form-group row">
    <label asp-for="desplegado" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.desplegado ? "Si" : "No")
        </p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="EditarProgramacion" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.EditarProgramacion ? "Si" : "No")
        </p>
    </div>
</div>

<div class="form-group row">
    <label asp-for="EditarEjecucion" class="col-sm-3 col-form-label"></label>
    <div class="col-sm-9">
        <p class="form-control-plaintext font-weight-light">
            @(Model.EditarEjecucion ? "Si" : "No")
        </p>
    </div>
</div>

<div class="form-group row">
    <div class="col-sm-3 offset-3">
        <a asp-action="Edit" asp-route-id="@Model.id" class="btn btn-outline-primary btn-block">Editar</a>
    </div>
    <div class="col-sm-3">
        <a asp-action="Index" class="btn btn btn-outline-dark btn-block">Volver</a>
    </div>
</div>